{
  "name": "chat",
  "version": "0.7.5",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "test": "node -r tsconfig-paths/register --test tests/**/*.test.cjs",
    "test:integration": "node -r tsconfig-paths/register --test tests/integration/*.test.cjs",
    "build": "npm run tsoa:gen && npm run tsoa:spec && rm -rf dist && npx tsc && tsc-alias",
    "tsoa:gen": "tsoa routes -c tsoa.config.json",
    "tsoa:spec": "tsoa spec -c tsoa.config.json",
    "start": "node dist/main.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/main.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "frontend:install": "cd frontend/chat-frontend && npm install -f && cd ../../ && cp -r static/* ./public",
    "frontend:build": "cd frontend/chat-frontend && npm run build && rm -rf ../../public/* && cp -r dist/* ../../public/ && cd ../../ && npm run tsoa:spec",
    "frontend:build:debug": "cd frontend/chat-frontend && npx vite build --mode development --sourcemap && rm -rf ../../public/* && cp -r dist/* ../../public/",
    "format": "npx prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "monitoring:start": "docker-compose -f docker-compose.monitoring.yml up -d",
    "monitoring:stop": "docker-compose -f docker-compose.monitoring.yml down",
    "monitoring:logs": "docker-compose -f docker-compose.monitoring.yml logs -f",
    "monitoring:start:production": "docker-compose -f docker-compose.monitoring.production.yml up -d",
    "monitoring:stop:production": "docker-compose -f docker-compose.monitoring.production.yml down",
    "monitoring:logs:production": "docker-compose -f docker-compose.monitoring.production.yml logs -f"
  },
  "keywords": [],
  "author": "Catflare <catty@example.com>",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.14.2",
    "@grpc/proto-loader": "^0.8.0",
    "@types/bcrypt": "^6.0.0",
    "@types/dotenv": "^8.2.3",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^2.0.0",
    "@types/sharp": "^0.31.1",
    "@types/socket.io": "^3.0.2",
    "@types/web-push": "^3.6.4",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "docker-compose": "^1.3.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "esbuild": "^0.25.12",
    "express": "^4.21.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^8.1.0",
    "inversify": "^6.2.2",
    "ipaddr.js": "^2.2.0",
    "jsdom": "^27.3.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.562.0",
    "mongodb": "^7.0.0",
    "mongoose": "^8.19.3",
    "multer": "^2.0.2",
    "prom-client": "^15.1.3",
    "recharts": "^3.6.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^7.0.0-alpha.0",
    "undici": "^7.16.0",
    "web-push": "^3.6.7",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.12"
  },
